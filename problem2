package com.Task;
import java.util.Scanner;

public class Problem2 {
public static void main(String[] args) {
	Scanner sc=new Scanner(System.in);
	System.out.println("Enter a input");
	int a=sc.nextInt();
	if(a<=4) {
	for(int i=1;i<=a;i++) {
	  int b=2*i-1;
	  System.out.print(b+",");
	}
	}
	else {
		System.out.print("1,3,5,7");
}

}
}
